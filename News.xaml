<Grid Margin="10,10,10,40" Height="40" Width="300">
<Grid.Triggers>
	<EventTrigger RoutedEvent="Loaded" >
        <BeginStoryboard>
			<Storyboard>
				<ColorAnimation
					Storyboard.TargetName="view"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					To="#777777" Duration="0:0:0.0"/>
				<ColorAnimation
					Storyboard.TargetName="viewBdr"
					Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
					To="#BBFFFFFF"/>
				<ColorAnimation
					Storyboard.TargetName="viewBdr"
					Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
					To="#777777"/>
			</Storyboard>
        </BeginStoryboard>
    </EventTrigger>
	<EventTrigger RoutedEvent="MouseEnter" >
        <BeginStoryboard>
			<Storyboard>
				<ColorAnimation
					Storyboard.TargetName="view"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
					To="{Binding}" Duration="0:0:0.25"/>
				<ColorAnimation
					Storyboard.TargetName="viewBdr"
					Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
					To="{Binding}" Duration="0:0:0.25"/>
				<DoubleAnimation
					Storyboard.TargetName="viewBdr"
					Storyboard.TargetProperty="Width"
					To="260" Duration="0:0:0.1"/>
				<ColorAnimation
					Storyboard.TargetName="viewBdr"
					Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
					To="{Binding}" Duration="0:0:0.25"/>
			</Storyboard>
        </BeginStoryboard>
    </EventTrigger>
	<EventTrigger RoutedEvent="MouseLeave" >
        <BeginStoryboard>
			<Storyboard>
				<ColorAnimation
					Storyboard.TargetName="view"
					Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
