<ListItem><Paragraph Style="{StaticResource Lp}"><TextBox Style="{StaticResource InlineCode}">panic</TextBox></Paragraph></ListItem>
<ListItem><Paragraph Style="{StaticResource Lp}"><TextBox Style="{StaticResource InlineCode}">pre_raid</TextBox></Paragraph></ListItem>
<ListItem><Paragraph Style="{StaticResource Lp}"><TextBox Style="{StaticResource InlineCode}">raid</TextBox></Paragraph></ListItem>
<ListItem><Paragraph Style="{StaticResource Lp}"><TextBox Style="{StaticResource InlineCode}">rest</TextBox></Paragraph></ListItem>
<ListItem><Paragraph Style="{StaticResource Lp}"><TextBox Style="{StaticResource InlineCode}">work</TextBox></Paragraph></ListItem>
